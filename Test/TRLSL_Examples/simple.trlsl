
struct VS_INPUT
{
    vec3 pos;
    vec4 color;
};

struct VS_OUTPUT
{
    vec3 pos;
};


vec3 func(vec3 a)
{
    return a;
}


// VS

struct VS_OUTPUT VS_main(struct VS_INPUT input)
{
    struct VS_OUTPUT output;

    output.pos = input.pos;

    func(input.pos);
    
    vec3 res = func(input.pos);

    return output;
}


// FS

vec4 FS_main(struct VS_OUTPUT input)
{
    return input.pos;
}